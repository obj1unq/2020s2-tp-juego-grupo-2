import wollok.game.*
import configuracion.*
import eventos.*
import musica.*

program game {
	
	
	//Configuracion inicial
	configurar.board()
	configurar.juego()
	configurar.teclas()
	game.addVisual(evento_inicioDelJuego)
	game.schedule(0, { musica.playCronica() } )	

	//Comienzo del juego
	game.start()
}